  RowBox[{"=", 
   RowBox[{
    FractionBox["1", "A"], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["1", "\[Gamma]"]}], "+", "\[Theta]", "-", "\[Theta]1", "+", 
      FractionBox[
       RowBox[{"\[Theta]", "-", "\[Theta]1"}], 
       RowBox[{
        RowBox[{"-", "1"}], "+", 
